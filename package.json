{
  "name": "multifarming",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy-kovanESW": "npx hardhat run scripts/deploy1TESW.js --network kovan",
    "deploy-kovanLP": "npx hardhat run scripts/deploy1TLP.js --network kovan",
    "deploy-kovanUSDT": "npx hardhat run scripts/deploy1TUSDT.js --network kovan",
    "prettier-test": "npx prettier --write test --print-width 120 --tab-width 4",
    "prettier-scripts": "npx prettier --write scripts --print-width 120 --tab-width 4",
    "prettier-sol": "prettier --write \"contracts/**/*.sol\" --print-width 120",
    "contract-flatten": "npx hardhat flatten contracts/RewardPool.sol > flat/RewardPool.flat.sol"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.4.0",
    "hardhat": "^2.6.7"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16"
  }
}
